<script>
    $(".religion").select2({
        tags: true,
    })
    $(".ms").select2({
        tags: true
    })
    $(".mt").select2({
        tags: true
    })
    $(".st").select2({
        tags: true
    })
    $(".ct").select2({
        tags: true
    })
    $(function () {
        $("#slider-range").slider({
            range: true,
            min: 18,
            max: 75,
            values: [20, 30],
            slide: function (event, ui) {
                $("#age1").val((ui.values[0]) + " - " + ui.values[1]);
            }
        });
        $("#age1").val($("#slider-range").slider("values", 0) +
            " - " + $("#slider-range").slider("values", 1));
    });
</script>


