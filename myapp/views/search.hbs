    <div class="container mt-5  ">
        <div class="row">
            <div class="col-md-7 offset-md-2 border rounded bg-light shadow p-3 px-5">
                <h2 class="text-center font-weight-light">Search</h2>
                <form action="/searchsave" method="POST" class="form-group">

                    <p>
                        <label for="age">Age range:</label>
                        <input type="text" name="age" id="age1">
                    </p>
                    <div id="slider-range"></div>
                    <div class="mb-3">
                        <label>Marital Status:</b></label>
                        <select placeholder="Marital Status" class="form-control ms" multiple="multiple"
                            name="maritialstatus">
                            <option value="nevermarried">Never Married</option>
                            <option value="divorced">Divorced</option>
                            <option value="windowed">Widowed</option>
                            <option value="awaitingdivorce">Awaiting Divorce</option>
                            <option value="anulled">Annulled</option>
                        </select>
                    </div>

                    <div class="mb-3">
                        <label>Religion:</b></label>
                        <select class="form-control religion" multiple="multiple" name="religion">
                            <option value="doesnotmatter">Doesn't Matter</option>
                            <option value="hindu">Hindu</option>
                            <option value="muslim">Islam</option>
                            <option value="buddhism">Buddhism</option>
                            <option value="christian">Christian</option>
                            <option value="jainism">Jainism</option>
                            <option value="judaism">Judaism</option>
                            <option value="sikhism">Sikhism</option>
                            <option value="taoism">Taoism</option>
                            <option value="zoroastrianism">Zoroastrianism</option>
                        </select>
                    </div>
                    <div class="mb-3">
                        <label>Mother Tongue</b></label>
                        <select class="form-control mt" multiple="multiple" name="mothertongue">
                            <option value="doesnotmatter">Doesn't Matter</option>
                            <option value="Hindi">Hindi</option>
                            <option value="bengali">Bengali</option>
                            <option value="urdu">Urdu</option>
                            <option value="punjabi">Punjabi</option>
                            <option value="marathi">Marathi</option>
                            <option value="telugu">Telugu</option>
                            <option value="tamil">Tamil</option>
                            <option value="gujarati">Gujarati</option>
                            <option value="oriya">Oriya</option>
                            <option value="Kannada">Kannada</option>
                            <option value="malayalam">Malayalam</option>
                            <option value="sanskrit">Sanskrit</option>
                        </select>
                    </div>
                    <div class=" mb-3">
                        <label for="countryId">Country</label>
                        <select name="country" class="countries custom-select" id="countryId">
                            <option value="">Select Country</option>
                            <option class="country" value="India">India</option>
                        </select>
                    </div>
                    <div class="mb-3 location">
                        <label for="stateId">State</label>
                        <select name="state" class="states custom-select st" multiple="multiple" id="stateId">
                            <option value="">Select State</option>
                        </select>
                    </div>
                    <div class="mb-3">
                        <label for="cityId">City</label>
                        <select name="city" class="cities custom-select ct" multiple="multiple" id="cityId">
                            <option value="">Select City</option>
                        </select>
                    </div>
                    <div>
                        <label>Photo Settings</b></label>
                        <div class="custom-control custom-checkbox mb-3">
                            <input type="checkbox" class="custom-control-input" id="customCheck" name="show" value="0">
                            <label class="custom-control-label" for="customCheck">Visible to All</label>
                        </div>
                        <div class="custom-control custom-checkbox mb-3">
                            <input type="checkbox" class="custom-control-input" id="customCheck_1" name="show"
                                value="1">
                            <label class="custom-control-label" for="customCheck_1">Protected Photo</label>
                        </div>



                    </div>
                    <div>
                        <button type="submit" class="btn btn-wine mt-3">Search</button>
                    </div>
                </form>

            </div>
        </div>
    </div>
